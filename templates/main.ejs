        </div>
    <!--EMPTY SIDE BLOKS-->
        <div class="out1"></div>
        <div class="out2"></div>
    <!--SIDE-INFO-->
        <div class="side">
        <!--MAIN INFO-->
            <%- include("./parts/info.ejs") %>
        <!--SKILLS-->
            <!--see for me-->
            <% if((permissEdit === 'true') && (vskillsme === 'on')) {%>
                <div class="side_sub">         
                    <% if(permissEdit === 'true') {%>
                        <p id="town"><%=name %> <%=surname %></p>
                    <% } %>  
                    <p>fghfgh</p>
                    <p>fghfghf</p>
                    <p>fghfghfggfhf</p>
                    <p>fghfghfghfghfghfg</p>
                    <p>fghfgh</p>              
                </div>
            <% } %>
            <!--see for all-->
            <% if((permissEdit === 'false') && (vskillsall === 'on')) {%>
                <div class="side_sub">  
                    <p>fghfgh</p>
                    <p>fghfghf</p>
                    <p>fghfghfggfhf</p>
                    <p>fghfghfghfghfghfg</p>
                    <p>fghfgh</p> 
                </div>
            <% } %>

        <!--PROJECTS-->
            <!--see for me-->
            <% if((permissEdit === 'true') && (vprojectsme === 'on')) {%>
                <div class="side_sub">         
                    <% if(permissEdit === 'true') {%>
                        <p id="town"><%=name %> <%=surname %></p>
                    <% } %>  
                    <p>fghfgh</p>
                    <p>fghfghf</p>
                </div>
            <% } %>
            <!--see for all-->
            <% if((permissEdit === 'false') && (vprojectsall === 'on')) {%>
                <div class="side_sub">  
                    <p>fghfgh</p>
                    <p>fghfghf</p>
                </div>
            <% } %>

        </div>
    <!--CONTENT-->
        <div class="article">    
            <%- include("./parts/article.ejs") %> 
        </div>
    <!--BLOK ON TOP FOOTER-->
        <div class="blok">
            <%- include("./parts/ontopfooter.ejs") %>
        </div>       
    </div>
<!--FOOTER-->
    <div class="footer-main-info">
        <%- include("./parts/footer.ejs") %>
    </div>
    
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <script src="./js/service.js"></script>
    <script src="./js/view.js"></script>
    <script src="./js/controller.js"></script>
    <% if(permissEdit === 'true') {%>
        <script>
            //file------------------------------------------------------------------------------------
            SE.$("reg-file").addEventListener("change", () => {VW.updateAva()});
        </script>
    <% } %>

</body>
